.de FILLED_BOX
.\" FILLED BOX Macro
.\" syntax:
.\" .FILLED_BOX string thickness outline_color fill_color text_color
.nr @wd \w'\\$1'
.gcolor \\$3
.fcolor \\$4
\h'-.4m'\v'(.4m - \\n[rsb]u)'
\Z'\D't \\$2p''\
\Z'\D'P (\\n[@wd]u + .8m) 0 0 -(\\n[rst]u - \\n[rsb]u + .8m) -(\\n[@wd]u + .8m) 0 0 (\\n[rst]u - \\n[rsb]u + .8m)''\
